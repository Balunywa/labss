                    <type>
                      <member>static</member>
                    </type>
                  </filter>
                  <priority>1</priority>
                  <action>
                    <redist/>
                  </action>
                </entry>
              </redist-profile>
            </protocol>
            <interface>
              <member>ethernet1/1</member>
              <member>tunnel.1</member>
            </interface>
            <routing-table>
