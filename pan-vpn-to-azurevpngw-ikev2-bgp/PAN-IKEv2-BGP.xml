                <dh-group>
                  <member>group20</member>
                </dh-group>
                <lifetime>
                  <hours>8</hours>
                </lifetime>
              </entry>
              <entry name="Azure-Profile">
                <hash>
                  <member>sha256</member>
                </hash>
                <dh-group>
                  <member>group2</member>
                </dh-group>
