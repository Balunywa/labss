                <router-id>192.168.2.1</router-id>
                <local-as>65002</local-as>
                <install-route>yes</install-route>
                <enforce-first-as>no</enforce-first-as>
                <redist-rules>
                  <entry name="static">
                    <address-family-identifier>ipv4</address-family-identifier>
                    <route-table>unicast</route-table>
                    <enable>yes</enable>
                    <set-origin>incomplete</set-origin>
                  </entry>
                </redist-rules>
              </bgp>
              <rip>
                <enable>no</enable>
              </rip>
              <ospf>
                <enable>no</enable>
              </ospf>
              <ospfv3>
                <enable>no</enable>
              </ospfv3>
              <redist-profile>
                <entry name="static">
