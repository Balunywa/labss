                <entry name="class5">
                  <priority>medium</priority>
                </entry>
                <entry name="class6">
                  <priority>low</priority>
                </entry>
                <entry name="class7">
                  <priority>low</priority>
                </entry>
                <entry name="class8">
                  <priority>low</priority>
                </entry>
              </class>
            </entry>
          </profile>
        </qos>
        <virtual-router>
          <entry name="Untrust">
            <ecmp>
              <algorithm>
                <ip-modulo/>
              </algorithm>
            </ecmp>
            <protocol>
              <bgp>
                <routing-options>
                  <graceful-restart>
                    <enable>yes</enable>
                  </graceful-restart>
                  <aggregate>
                    <aggregate-med>no</aggregate-med>
                  </aggregate>
                  <med>
                    <deterministic-med-comparison>no</deterministic-med-comparison>
                  </med>
                </routing-options>
                <enable>yes</enable>
                <peer-group>
                  <entry name="BGP">
