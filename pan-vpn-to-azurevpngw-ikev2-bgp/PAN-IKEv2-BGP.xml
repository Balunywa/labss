                    <bfd>
                      <profile>None</profile>
                    </bfd>
                    <path-monitor>
                      <enable>no</enable>
                      <failure-condition>any</failure-condition>
                      <hold-time>2</hold-time>
                    </path-monitor>
                    <interface>ethernet1/2</interface>
                    <metric>10</metric>
                    <destination>10.1.10.0/24</destination>
                    <route-table>
                      <unicast/>
                    </route-table>
                  </entry>
                  <entry name="to-azure">
                    <path-monitor>
                      <enable>no</enable>
                      <failure-condition>any</failure-condition>
                      <hold-time>2</hold-time>
                    </path-monitor>
                    <nexthop>
                      <next-vr>Untrust</next-vr>
